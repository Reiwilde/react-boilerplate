{
  "parser": "eslint-plugin-typescript/parser",
  "plugins": [
    "typescript"
  ],
  "extends": [
    "standard",
    "standard-jsx"
  ],
  "rules": {
    "no-unused-vars": "off",
    "typescript/no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ]
  }
}
